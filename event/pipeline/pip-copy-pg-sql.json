{
	"name": "pip-copy-pg-sql",
	"properties": {
		"activities": [
			{
				"name": "VercelPostgressToAzureDatabase",
				"description": "Copy 4 table\nUser\nOrder\nCategory\nEvent",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "PostgreSqlV2Source"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_postgress_sql_get_table",
						"type": "DatasetReference",
						"parameters": {
							"schemaName": "public",
							"tableName": "User"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_azure_sql_create_table_sink",
						"type": "DatasetReference",
						"parameters": {
							"schemaName": "raw",
							"tableName": "User"
						}
					}
				]
			}
		],
		"folder": {
			"name": "event"
		},
		"annotations": []
	}
}